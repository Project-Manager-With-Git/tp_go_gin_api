{
    "language": "go",
    "env": "gomod",
    "description": "gin简单api服务的模板,注意使用`swag init --parseDependency --parseInternal`创建docs文件夹",
    "author": "hsz",
    "template_type": "socket",
    "requires": [
        "github.com/Golang-Tools/jwthelper@v0.0.4",
        "github.com/Golang-Tools/loggerhelper@v0.0.4",
        "github.com/Golang-Tools/redishelper@v0.0.7",
        "github.com/Golang-Tools/schema-entry-go@v0.0.7",
        "github.com/Golang-Tools/sqlhelper@v0.0.7",
        "github.com/alecthomas/jsonschema@v0.0.0-20210920000243-787cd8204a0d",
        "github.com/gin-contrib/cors@v1.3.1",
        "github.com/gin-contrib/static@v0.0.1",
        "github.com/gin-gonic/gin@v1.7.4",
        "github.com/scylladb/go-set@v1.0.2",
        "github.com/swaggo/files@v0.0.0-20210815190702-a29dd2bc99b2",
        "github.com/swaggo/gin-swagger@v1.3.1",
        "github.com/toorop/gin-logrus@v0.0.0-20210225092905-2c785434f26f",
        "github.com/uptrace/bun@v1.0.8",
        "github.com/xeipuuv/gojsonschema@v1.2.0"
    ],
    "template_keys": {
        "app_name": {
            "description": "应用名",
            "default": "{{ project_name }}"
        },
        "api_name": {
            "description": "应用名",
            "default": "ping"
        },
        "source": {
            "description": "资源名",
            "default": "student"
        },
        "author": {
            "description": "作者",
            "default": "{{ author }}"
        },
        "author_email": {
            "description": "作者",
            "default": "{{ author_email }}"
        },
        "version": {
            "description": "项目版本",
            "default": "{{ version }}"
        }
    },
    "test": {
        "source": "cp_go_gin@v0.0.2//apitest",
        "default_path": "servtest/apitest"
    },
    "components": {
        "auth": {
            "source": "cp_go_gin@v0.0.2//simple_auth",
            "description": "简易权限验证模块",
            "default_path": "auth"
        },
        "apis": {
            "source": "cp_go_gin@v0.0.2//apis",
            "description": "RESTfulapi的接口模板",
            "default_path": "apis"
        },
        "models": {
            "source": "cp_go_gin@v0.0.2//models",
            "description": "数据模型模板",
            "default_path": "models"
        },
        "serv": {
            "source": "serv",
            "description": "服务器存放位置",
            "default_path": "serv"
        },
        "main": {
            "source": "cp_go_gin@v0.0.2//main",
            "description": "入口函数位置",
            "default_path": "main.go"
        },
        "docker-compose": {
            "source": "cp_dockercompose@v0.0.2//server_standalone",
            "description": "通用的服务使用的dockercompose,适用于调试",
            "default_path": "docker-compose.yml"
        },
        "dockerfile": {
            "source": "cp_dockerfiles@v0.0.1//go_http_pure_dockerfile",
            "description": "构造gin项目的dockerfile",
            "default_path": "dockerfile"
        }
    }
}